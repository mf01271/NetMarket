<script>
    $(document).ready(function () {
        $(".btnaddlist").click(function () {
            var idpro = $(this).attr("data-idproducto");
            var datos3 = { idProductoSucursal: idpro };
            var _datosProductoCategoria;
            $.ajax({
                url: "http://localhost/ApiNet/api/productosucursal/verproducto",
                cache: false,
                data: JSON.stringify(datos3),
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                dataType: 'json'
            }).done(function (respuestaMVC) {
                console.log(respuestaMVC.Mensaje);
                if (respuestaMVC.Codigo == 0) {
                    _datosProductoCategoria = respuestaMVC.Data;
                    console.log(_datosProductoCategoria);
                    mostrarDatosCategorias();
                } else {
                    console.log(respuestaMVC.Mensaje);
                }
            });
            function mostrarDatosCategorias() {
                //cargar galeria de imagenes
                var imgprincipal = "thumb-item-01.jpg";
                var imgsecundaria = "thumb-item-01.jpg";
                var imgterciaria = "thumb-item-01.jpg";
                if (_datosProductoCategoria.rutaimagen1 != "") {
                    imgprincipal = _datosProductoCategoria.rutaimagen1;
                } else {
                    imgprincipal = "thumb-item-01.jpg";
                }
                if (_datosProductoCategoria.rutaimagen2 != "") {
                    imgsecundaria = _datosProductoCategoria.rutaimagen2;
                } else {
                    imgsecundaria = "thumb-item-01.jpg";
                }
                if (_datosProductoCategoria.rutaimagen3 != "") {
                    imgterciaria = _datosProductoCategoria.rutaimagen3;
                } else {
                    imgterciaria = "thumb-item-01.jpg";
                }
                $("#prodcname").text(_datosProductoCategoria.nombre);
                $("#prodcdesc").text(_datosProductoCategoria.descripcionCorta);
                $("#imgprinc1").attr("data-thumb", "http://localhost/NetMarket/images/" + imgprincipal);
                var lithumb = $(".wrap-slick3-dots .slick3-dots li");
                //console.log(lithumb);
                lithumb[0].innerHTML = "<img src='http://localhost/NetMarket/images/" + imgprincipal + "'><div class='slick3-dot-overlay'></div>";
                $("#imgprinc2").attr("src", "http://localhost/NetMarket/images/" + imgprincipal);
                var cat = $("#prodccate").text();
                cat = cat + _datosProductoCategoria.nombreCategoria;
                $("#prodccate").text(cat);
                $("#prodcdesclong").text(_datosProductoCategoria.descripcionLarga);
                $("#prodcprecio").text("Bs. " + _datosProductoCategoria.precio);
                $("#btnaddList").attr("data-idprodcsu", _datosProductoCategoria.idProductoSucursal);
                $("#btnaddList").click(function () {
                    var idprosu = $(this).attr("data-idprodcsu");
                    var varcant = $("#cantinput").val();
                    if (idprosu != 0) {
                        var data = {
                            idProducto: idprosu,
                            Cantidad: varcant
                        };
                        $.ajax({
                            url: "@Url.Action("AgregaralCarrito", "Carrito")",
                            cache: false,
                            data: JSON.stringify(data),
                            type: "POST",
                            contentType: 'application/json; charset=utf-8',
                            dataType: 'json'
                        }).done(function (res) {
                            console.log(res.productos);
                            sweetAlert("Mensaje", res.mensaje, "success");
                        });
                        
                    }
                    else {
                        sweetAlert("Mensaje","Producto no disponible","success");
                    }
                });

            }
        });
    });
</script>
<script>
